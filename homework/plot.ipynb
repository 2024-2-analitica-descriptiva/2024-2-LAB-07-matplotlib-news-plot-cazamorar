    "df = pd.read_csv(\"files/input/news.csv\", index_col=0)\n",
    "    #print(df.head())\n",
    "\n",
    "for col in df.columns:\n",
    "    plt.plot(df[col], label=col)\n",
    "\n",
    "plt.title(\"How people get their news\", fontsize=16)\n",
    "plt.show"
